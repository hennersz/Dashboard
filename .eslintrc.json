{
    "env": {
        "es6": true,
        "node": true,
        "commonjs": true,
        "jest": true
    },
    "extends": [
        "google",
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-console":"off",
        "no-var": "error"
    }
}